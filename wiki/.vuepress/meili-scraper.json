{
  "index_uid": "wiki",

  "start_urls": ["https://zenithworld.top/"],
  "sitemap_urls": ["https://zenithworld.top/sitemap.xml"],

  "selectors": {
    "lvl0": {
      "selector": ".vp-sidebar-heading.active, .vp-breadcrumb .active, head title",
      "global": true,
      "default_value": "Wiki"
    },

    "lvl1": "main h1, [vp-content] h1, article h1, .theme-hope-content h1",

    "text": "main p, main li, [vp-content] p, [vp-content] li",

    "lang": {
      "selector": "/html/@lang",
      "global": true,
      "type": "xpath"
    }
  },

  "custom_settings": {
    "searchableAttributes": [
      "hierarchy_radio_lvl0",
      "hierarchy_radio_lvl1",
      "hierarchy_radio_lvl2",
      "hierarchy_radio_lvl3",
      "hierarchy_radio_lvl4",
      "hierarchy_radio_lvl5",
      "hierarchy_lvl0",
      "hierarchy_lvl1",
      "hierarchy_lvl2",
      "hierarchy_lvl3",
      "hierarchy_lvl4",
      "hierarchy_lvl5",
      "hierarchy_lvl6",
      "text",
      "lang",
      "objectID"
    ],
    "displayedAttributes": [
      "hierarchy_lvl0",
      "hierarchy_lvl1",
      "hierarchy_lvl2",
      "hierarchy_lvl3",
      "hierarchy_lvl4",
      "hierarchy_lvl5",
      "hierarchy_lvl6",
      "anchor",
      "url",
      "text",
      "lang"
    ],
    "filterableAttributes": ["lang"],
    
    "sortableAttributes": [
      "updatedAt",
      "viewCount"
    ]
  }
}