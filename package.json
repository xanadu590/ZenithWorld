  "license": "MIT",
  "type": "module",
  "scripts": {
    "docs:gen-index": "node wiki/.vuepress/scripts/gen-random-index.mjs",
    "docs:dev": "pnpm run docs:gen-index && vuepress-vite dev wiki",
    "docs:build": "pnpm run docs:gen-index && vuepress-vite build wiki",
    "docs:clean-dev": "rimraf wiki/.vuepress/.cache wiki/.vuepress/.temp && pnpm run docs:dev",
    "docs:update-package": "pnpm dlx vp-update"
  },
  "devDependencies": {
    "@vue/compiler-dom": "^3.5.25",
    "@vuepress/bundler-vite": "2.0.0-rc.24",
    "@vuepress/client": "2.0.0-rc.24",
    "@vuepress/core": "2.0.0-rc.24",
    "@vuepress/plugin-git": "2.0.0-rc.112",
    "@vuepress/plugin-meilisearch": "2.0.0-rc.118",
    "@vuepress/plugin-photo-swipe": "2.0.0-rc.112",
    "@vuepress/plugin-slimsearch": "2.0.0-rc.112",
    "@vuepress/utils": "2.0.0-rc.24",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "meilisearch": "^0.54.0",
    "rimraf": "^6.0.1",
    "sass-embedded": "^1.93.2",
    "vue": "^3.5.22",
    "vuepress": "2.0.0-rc.24",
    "vuepress-theme-hope": "2.0.0-rc.94"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
